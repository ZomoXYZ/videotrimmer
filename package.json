{
    "name": "videotrimmer",
    "version": "1.1.0",
    "license": "MIT",
    "description": "easy video trimmer",
    "author": {
        "name": "ashley zomo",
        "email": "ashleyzomo@gmail.com"
    },
    "main": "dist/API/main.js",
    "scripts": {
        "build": "node ./scripts/build.js -s",
        "start-only": "electron .",
        "watch": "node ./scripts/build.js -sw",
        "start": "npm run build && electron .",
        "build-all": "node ./scripts/build.js -smb",
        "start-all": "npm run build-all && electron .",
        "prebuild": "node -p \"'// autogenerated by prebuild script in package.json\\nexport const APP_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/UI/util/version.ts",
        "build-electron": "npm run build-all && electron-builder build",
        "prettier": "prettier --write ./src/UI",
        "start-ui": "vite --mode development"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ZomoXYZ/videotrimmer.git"
    },
    "keywords": [
        "video",
        "trimmer",
        "video-trimmer",
        "electron",
        "electron-app",
        "nvidia-shadowplay"
    ],
    "bugs": {
        "url": "https://github.com/ZomoXYZ/videotrimmer/issues"
    },
    "homepage": "https://github.com/ZomoXYZ/videotrimmer#readme",
    "devDependencies": {
        "@types/mime-db": "^1.43.1",
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.14",
        "@types/rimraf": "^3.0.2",
        "command-line-args": "^5.2.1",
        "electron": "^19.0.6",
        "electron-builder": "^23.1.0",
        "esbuild": "^0.14.47",
        "prettier": "^2.7.1",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@babel/core": "^7.18.5",
        "@emotion/css": "^11.9.0",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@types/ffbinaries": "^1.1.2",
        "@types/fluent-ffmpeg": "^2.1.20",
        "@types/mime": "^2.0.3",
        "@types/react-dom": "^18.0.5",
        "@vitejs/plugin-react": "^1.3.2",
        "electron-log": "^4.4.8",
        "electron-updater": "^5.0.5",
        "ffbinaries": "^1.1.4",
        "fluent-ffmpeg": "^2.1.2",
        "mime": "^3.0.0",
        "mime-db": "^1.52.0",
        "npm": "^8.13.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0",
        "rimraf": "^3.0.2",
        "vite": "^2.9.12"
    }
}
