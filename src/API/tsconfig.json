{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "../../dist/API",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "typeRoots": ["./types/"]
  },
  "include": ["**/*"]
}
