{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "../../dist/UI",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "typeRoots": [
      "./types/"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "include": [
    "**/*"
  ]
}
